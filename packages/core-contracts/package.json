{
  "name": "@livai/core-contracts",
  "version": "1.0.0",
  "description": "Core Contracts - Foundation layer for effects, auth, domain, errors, and infrastructure contracts",
  "repository": {
    "type": "git",
    "url": "https://github.com/livai/livai.git",
    "directory": "packages/core-contracts"
  },
  "bugs": {
    "url": "https://github.com/livai/livai/issues"
  },
  "homepage": "https://github.com/livai/livai/tree/main/packages/core-contracts#readme",
  "main": "./dist/esm/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": "24.12.0",
    "pnpm": "10.28.2"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./io": {
      "types": "./dist/esm/io/index.d.ts",
      "import": "./dist/esm/io/index.js",
      "default": "./dist/esm/io/index.js"
    },
    "./fp": {
      "types": "./dist/esm/fp/index.d.ts",
      "import": "./dist/esm/fp/index.js",
      "default": "./dist/esm/fp/index.js"
    },
    "./domain": {
      "types": "./dist/esm/domain/index.d.ts",
      "import": "./dist/esm/domain/index.js",
      "default": "./dist/esm/domain/index.js"
    },
    "./errors": {
      "types": "./dist/esm/errors/index.d.ts",
      "import": "./dist/esm/errors/index.js",
      "default": "./dist/esm/errors/index.js"
    },
    "./context": {
      "types": "./dist/esm/context/index.d.ts",
      "import": "./dist/esm/context/index.js",
      "default": "./dist/esm/context/index.js"
    },
    "./targets/shared": {
      "types": "./dist/esm/targets/shared.d.ts",
      "import": "./dist/esm/targets/shared.js",
      "default": "./dist/esm/targets/shared.js"
    },
    "./targets/node": {
      "types": "./dist/esm/targets/node.d.ts",
      "import": "./dist/esm/targets/node.js",
      "default": "./dist/esm/targets/node.js"
    },
    "./targets/browser": {
      "types": "./dist/esm/targets/browser.d.ts",
      "import": "./dist/esm/targets/browser.js",
      "default": "./dist/esm/targets/browser.js"
    },
    "./validation/zod": {
      "types": "./dist/esm/validation/zod/index.d.ts",
      "import": "./dist/esm/validation/zod/index.js",
      "default": "./dist/esm/validation/zod/index.js"
    }
  },
  "scripts": {
    "build": "pnpm run build:types & pnpm run build:js & wait",
    "build:js": "tsup",
    "build:types": "tsc -b tsconfig.build.json",
    "build:watch": "tsup --watch",
    "dev": "tsup --watch",
    "lint": "pnpm run lint:dev",
    "type-check": "tsc --noEmit",
    "test": "NODE_OPTIONS='--no-warnings' vitest run",
    "test:watch": "vitest --watch",
    "test:update": "NODE_OPTIONS='--no-warnings' vitest run -u",
    "test:ui": "NODE_OPTIONS='--no-warnings' vitest --ui",
    "test:coverage": "NODE_OPTIONS='--no-warnings' vitest run --coverage",
    "test:coverage:html": "NODE_OPTIONS='--no-warnings' vitest run --coverage --coverage.reporter=html",
    "test:coverage:watch": "NODE_OPTIONS='--no-warnings' vitest --coverage --watch",
    "test:ci": "NODE_OPTIONS='--no-warnings' vitest run --coverage",
    "clean": "rm -rf dist *.tsbuildinfo .eslintcache",
    "lint:dev": "ESLINT_MODE=dev eslint --config ../../eslint.config.mjs src",
    "lint:canary": "ESLINT_MODE=canary eslint --config ../../eslint.config.mjs src",
    "lint:canary:fix": "ESLINT_MODE=canary eslint --config ../../eslint.config.mjs src --fix",
    "check:deps": "cd ../.. && node scripts/check-circular-deps-monorepo.js",
    "ci:check": "pnpm run type-check && pnpm run lint && pnpm run test:ci && pnpm run check:deps",
    "lint:fix": "ESLINT_MODE=dev eslint --config ../../eslint.config.mjs src --fix",
    "lint:ci": "ESLINT_MODE=canary eslint --config ../../eslint.config.mjs src --max-warnings 0",
    "generate:openapi": "PYTHONDONTWRITEBYTECODE=1 python3 ../../scripts/generate-openapi-snapshots.py",
    "generate:zod": "node ../../scripts/generate-zod-schemas.mjs",
    "generate:contracts": "node -e \"process.exit(0)\"",
    "check:contracts": "git diff --exit-code -- ':(glob)services/*-service/openapi.json' 'packages/core-contracts/src/validation/zod/generated' || (echo \"\\n[contracts] contract drift detected\" && exit 1)",
    "bench": "NODE_OPTIONS='--no-warnings' vitest bench -c vitest.bench.config.ts --run",
    "bench:ci": "NODE_OPTIONS='--no-warnings' vitest bench -c vitest.bench.config.ts --run"
  },
  "keywords": [
    "typescript",
    "fp",
    "effect",
    "domain-driven-design",
    "contracts",
    "core",
    "foundation"
  ],
  "peerDependencies": {
    "effect": "^3.19.19",
    "typescript": "5.9.3"
  },
  "peerDependenciesMeta": {
    "effect": {
      "optional": true
    }
  },
  "devDependencies": {
    "@vitest/coverage-v8": "4.0.18",
    "eslint": "9.39.3",
    "openapi-zod-client": "1.18.3",
    "tsup": "8.5.1",
    "typescript": "5.9.3",
    "vitest": "4.0.18"
  },
  "tags": [
    "node",
    "critical"
  ],
  "dependencies": {
    "@effect/ai": "^0.33.0",
    "@effect/ai-amazon-bedrock": "^0.13.0",
    "@effect/ai-anthropic": "^0.23.0",
    "@effect/ai-google": "^0.12.0",
    "@effect/ai-openai": "^0.37.0",
    "@effect/cli": "^0.73.0",
    "@effect/cluster": "^0.56.4",
    "@effect/experimental": "^0.58.0",
    "@effect/platform": "^0.94.5",
    "@effect/platform-browser": "^0.74.0",
    "@effect/platform-bun": "^0.87.1",
    "@effect/platform-node": "0.104.1",
    "@effect/printer": "^0.47.0",
    "@effect/printer-ansi": "^0.47.0",
    "@effect/rpc": "^0.73.2",
    "@effect/rpc-http": "^0.52.4",
    "@effect/sql": "^0.49.0",
    "@effect/sql-clickhouse": "^0.46.0",
    "@effect/sql-d1": "^0.47.0",
    "@effect/sql-drizzle": "^0.48.0",
    "@effect/sql-kysely": "^0.45.0",
    "@effect/sql-libsql": "^0.39.0",
    "@effect/sql-mssql": "^0.50.0",
    "@effect/sql-mysql2": "^0.50.0",
    "@effect/sql-pg": "^0.50.3",
    "@effect/sql-sqlite-bun": "^0.50.0",
    "@effect/sql-sqlite-do": "^0.27.0",
    "@effect/sql-sqlite-node": "^0.50.0",
    "@effect/sql-sqlite-react-native": "^0.52.0",
    "@effect/sql-sqlite-wasm": "^0.50.0",
    "@effect/typeclass": "^0.38.0",
    "@effect/vitest": "^0.27.0",
    "@effect/workflow": "^0.16.0",
    "effect": "^3.19.19",
    "zod": "^4.3.6"
  },
  "optionalDependencies": {
    "lru-cache": "11.2.6"
  }
}
