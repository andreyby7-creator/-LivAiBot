{
  "name": "@livai/domains",
  "version": "1.0.0",
  "description": "Classification domain implementation - decision computing domain with labels, signals, strategies",
  "repository": {
    "type": "git",
    "url": "https://github.com/livai/livai.git",
    "directory": "packages/domains/classification"
  },
  "bugs": {
    "url": "https://github.com/livai/livai/issues"
  },
  "homepage": "https://github.com/livai/livai/tree/main/packages/domains/classification#readme",
  "main": "./dist/esm/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=24.0.0",
    "pnpm": "10.28.2"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./signals": {
      "types": "./src/classification/signals/index.ts",
      "import": "./src/classification/signals/index.ts"
    },
    "./strategies": {
      "types": "./src/classification/strategies/index.ts",
      "import": "./src/classification/strategies/index.ts"
    },
    "./policies": {
      "types": "./src/classification/policies/index.ts",
      "import": "./src/classification/policies/index.ts"
    },
    "./aggregation": {
      "types": "./src/classification/aggregation/index.ts",
      "import": "./src/classification/aggregation/index.ts"
    },
    "./labels": {
      "types": "./src/classification/labels.ts",
      "import": "./src/classification/labels.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "build:types": "tsc -b tsconfig.build.json",
    "build:dev": "tsup --sourcemap --no-clean",
    "build:watch": "tsup --watch",
    "dev": "tsup --watch",
    "lint": "pnpm run lint:dev",
    "type-check": "tsc --noEmit",
    "test": "NODE_OPTIONS='--no-warnings' vitest",
    "test:watch": "vitest --watch",
    "test:ui": "NODE_OPTIONS='--no-warnings' vitest --ui",
    "test:coverage": "NODE_OPTIONS='--no-warnings' vitest run --coverage",
    "test:coverage:html": "NODE_OPTIONS='--no-warnings' vitest run --coverage --coverage.reporter=html",
    "test:coverage:watch": "NODE_OPTIONS='--no-warnings' vitest --coverage --watch",
    "test:ci": "NODE_OPTIONS='--no-warnings' vitest run --coverage",
    "clean": "rm -rf dist *.tsbuildinfo .eslintcache",
    "lint:dev": "ESLINT_MODE=dev eslint --config ../../eslint.config.mjs src",
    "lint:canary": "ESLINT_MODE=canary eslint --config ../../eslint.config.mjs src",
    "lint:canary:fix": "ESLINT_MODE=canary eslint --config ../../eslint.config.mjs src --fix",
    "check:deps": "cd ../.. && node scripts/check-circular-deps-monorepo.js",
    "ci:check": "pnpm run type-check && pnpm run lint && pnpm run test:ci && pnpm run check:deps",
    "lint:fix": "ESLINT_MODE=dev eslint --config ../../eslint.config.mjs src --fix",
    "lint:ci": "ESLINT_MODE=canary eslint --config ../../eslint.config.mjs src --max-warnings 0"
  },
  "keywords": [
    "typescript",
    "domain-driven-design",
    "classification",
    "decision-computing",
    "core",
    "foundation"
  ],
  "peerDependencies": {
    "effect": "^3.19.19",
    "typescript": "5.9.3"
  },
  "peerDependenciesMeta": {
    "effect": {
      "optional": true
    }
  },
  "devDependencies": {
    "@livai/vitest-config": "workspace:*",
    "@vitest/coverage-v8": "4.0.18",
    "eslint": "9.39.3",
    "tsup": "8.5.1",
    "typescript": "5.9.3",
    "vitest": "4.0.18"
  },
  "tags": [
    "node",
    "critical",
    "domain",
    "foundation"
  ],
  "dependencies": {
    "@livai/core": "workspace:*"
  }
}
