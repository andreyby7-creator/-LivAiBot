{
  "name": "@livai/feature-voice",
  "version": "1.0.0",
  "description": "Voice feature business logic and domain models",
  "repository": {
    "type": "git",
    "url": "https://github.com/livai/livai.git",
    "directory": "packages/feature-voice"
  },
  "bugs": {
    "url": "https://github.com/livai/livai/issues"
  },
  "homepage": "https://github.com/livai/livai/tree/main/packages/feature-voice#readme",
  "main": "./dist/esm/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": "24.12.0",
    "pnpm": "10.28.2"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "build:dev": "tsup --sourcemap --no-clean",
    "build:watch": "tsup --watch",
    "dev": "tsup --watch",
    "lint": "pnpm run lint:dev",
    "type-check": "tsc --noEmit",
    "test": "NODE_OPTIONS='--no-warnings' vitest",
    "test:watch": "vitest --watch",
    "test:ui": "NODE_OPTIONS='--no-warnings' vitest --ui",
    "test:coverage": "NODE_OPTIONS='--no-warnings' vitest run --coverage",
    "test:coverage:html": "NODE_OPTIONS='--no-warnings' vitest run --coverage --coverage.reporter=html",
    "test:coverage:watch": "NODE_OPTIONS='--no-warnings' vitest --coverage --watch",
    "test:ci": "NODE_OPTIONS='--no-warnings' vitest run --coverage",
    "clean": "rm -rf dist *.tsbuildinfo .eslintcache",
    "lint:dev": "ESLINT_MODE=dev eslint --config ../../eslint.config.mjs src",
    "lint:canary": "ESLINT_MODE=canary eslint --config ../../eslint.config.mjs src",
    "lint:canary:fix": "ESLINT_MODE=canary eslint --config ../../eslint.config.mjs src --fix",
    "check:deps": "cd ../.. && node scripts/check-circular-deps-monorepo.js",
    "ci:check": "pnpm run type-check && pnpm run lint && pnpm run test:ci && pnpm run check:deps",
    "lint:fix": "ESLINT_MODE=dev eslint --config ../../eslint.config.mjs src --fix",
    "lint:ci": "ESLINT_MODE=canary eslint --config ../../eslint.config.mjs src --max-warnings 0"
  },
  "keywords": [
    "typescript",
    "fp",
    "effect",
    "domain-driven-design",
    "contracts",
    "core",
    "foundation"
  ],
  "peerDependencies": {
    "effect": "^3.19.15",
    "typescript": "5.9.3"
  },
  "peerDependenciesMeta": {},
  "devDependencies": {
    "@livai/vitest-config": "workspace:*",
    "@vitest/coverage-v8": "4.0.18",
    "eslint": "9.39.2",
    "tsup": "8.5.1",
    "typescript": "5.9.3",
    "vitest": "4.0.18"
  },
  "tags": [
    "node",
    "critical"
  ],
  "dependencies": {
    "@effect/ai": "^0.33.0",
    "@effect/ai-amazon-bedrock": "^0.13.0",
    "@effect/ai-anthropic": "^0.23.0",
    "@effect/ai-google": "^0.12.0",
    "@effect/ai-openai": "^0.37.0",
    "@effect/cli": "^0.73.0",
    "@effect/cluster": "^0.56.0",
    "@effect/experimental": "^0.58.0",
    "@effect/platform": "^0.94.2",
    "@effect/platform-browser": "^0.74.0",
    "@effect/platform-bun": "^0.87.0",
    "@effect/platform-node": "0.104.1",
    "@effect/printer": "^0.47.0",
    "@effect/printer-ansi": "^0.47.0",
    "@effect/rpc": "^0.73.0",
    "@effect/rpc-http": "^0.52.4",
    "@effect/sql": "^0.49.0",
    "@effect/sql-clickhouse": "^0.46.0",
    "@effect/sql-d1": "^0.47.0",
    "@effect/sql-drizzle": "^0.48.0",
    "@effect/sql-kysely": "^0.45.0",
    "@effect/sql-libsql": "^0.39.0",
    "@effect/sql-mssql": "^0.50.0",
    "@effect/sql-mysql2": "^0.50.0",
    "@effect/sql-pg": "^0.50.0",
    "@effect/sql-sqlite-bun": "^0.50.0",
    "@effect/sql-sqlite-do": "^0.27.0",
    "@effect/sql-sqlite-node": "^0.50.0",
    "@effect/sql-sqlite-react-native": "^0.52.0",
    "@effect/sql-sqlite-wasm": "^0.50.0",
    "@effect/typeclass": "^0.38.0",
    "@effect/vitest": "^0.27.0",
    "@effect/workflow": "^0.16.0",
    "effect": "^3.19.15"
  },
  "optionalDependencies": {
    "lru-cache": "11.2.4"
  }
}
