{
  "name": "@livai/app",
  "version": "1.0.0",
  "description": "Next.js composition",
  "repository": {
    "type": "git",
    "url": "https://github.com/livai/livai.git",
    "directory": "packages/app"
  },
  "bugs": {
    "url": "https://github.com/livai/livai/issues"
  },
  "homepage": "https://github.com/livai/livai/tree/main/packages/app#readme",
  "main": "./dist/esm/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=24.0.0",
    "pnpm": "10.28.2"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./lib/error-mapping.js": {
      "types": "./src/lib/error-mapping.ts",
      "import": "./src/lib/error-mapping.ts"
    },
    "./lib/effect-utils.js": {
      "types": "./src/lib/effect-utils.ts",
      "import": "./src/lib/effect-utils.ts"
    },
    "./lib/orchestrator.js": {
      "types": "./src/lib/orchestrator.ts",
      "import": "./src/lib/orchestrator.ts"
    },
    "./lib/effect-timeout.js": {
      "types": "./src/lib/effect-timeout.ts",
      "import": "./src/lib/effect-timeout.ts"
    },
    "./lib/service-worker.js": {
      "types": "./src/lib/service-worker.ts",
      "import": "./src/lib/service-worker.ts"
    },
    "./lib/telemetry-runtime.js": {
      "types": "./src/lib/telemetry-runtime.ts",
      "import": "./src/lib/telemetry-runtime.ts"
    },
    "./providers/intl-provider.js": {
      "types": "./src/providers/intl-provider.tsx",
      "import": "./src/providers/intl-provider.tsx"
    }
  },
  "scripts": {
    "build": "tsup",
    "build:types": "tsc -b tsconfig.build.json",
    "build:dev": "tsup --sourcemap --no-clean",
    "build:watch": "tsup --watch",
    "dev": "tsup --watch",
    "lint": "pnpm run lint:dev",
    "type-check": "tsc --noEmit",
    "test": "NODE_OPTIONS='--no-warnings' vitest",
    "test:watch": "vitest --watch",
    "test:ui": "NODE_OPTIONS='--no-warnings' vitest --ui",
    "test:coverage": "NODE_OPTIONS='--no-warnings' vitest run --coverage",
    "test:coverage:html": "NODE_OPTIONS='--no-warnings' vitest run --coverage --coverage.reporter=html",
    "test:coverage:watch": "NODE_OPTIONS='--no-warnings' vitest --coverage --watch",
    "test:ci": "NODE_OPTIONS='--no-warnings' vitest run --coverage --passWithNoTests=false",
    "clean": "rm -rf dist *.tsbuildinfo .eslintcache",
    "lint:dev": "ESLINT_MODE=dev eslint --config ../../eslint.config.mjs src",
    "lint:canary": "ESLINT_MODE=canary eslint --config ../../eslint.config.mjs src",
    "lint:canary:fix": "ESLINT_MODE=canary eslint --config ../../eslint.config.mjs src --fix",
    "check:deps": "cd ../.. && node scripts/check-circular-deps-monorepo.js",
    "ci:check": "pnpm run type-check && pnpm run lint && pnpm run test:ci && pnpm run check:deps",
    "lint:fix": "ESLINT_MODE=dev eslint --config ../../eslint.config.mjs src --fix",
    "lint:ci": "ESLINT_MODE=canary eslint --config ../../eslint.config.mjs src --max-warnings 0"
  },
  "keywords": [
    "typescript",
    "ui",
    "nextjs",
    "app",
    "providers"
  ],
  "peerDependencies": {
    "react": "19.2.4",
    "typescript": "5.9.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "16.3.2",
    "@vitest/coverage-v8": "4.0.18",
    "eslint": "9.39.3",
    "ioredis": "^5.9.3",
    "jsdom": "28.1.0",
    "react-dom": "19.2.4",
    "tsup": "8.5.1",
    "typescript": "5.9.3",
    "vitest": "4.0.18"
  },
  "tags": [
    "node",
    "critical",
    "ui"
  ],
  "dependencies": {
    "@livai/core-contracts": "workspace:*",
    "@livai/ui-core": "workspace:*",
    "@tanstack/react-query": "^5.90.21",
    "async-mutex": "^0.5.0",
    "dayjs": "1.11.19",
    "next-intl": "4.8.3",
    "uuid": "^13.0.0",
    "web-vitals": "^5.1.0",
    "zod": "^4.3.6"
  }
}
