{
  "name": "livai",
  "version": "1.0.0",
  "description": "LivAi - AI-powered chatbot platform with multi-tenant architecture",
  "packageManager": "pnpm@10.28.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "TURBO_FORCE=true TURBO_REMOTE_CACHE_DISABLED=true TURBO_CACHE_DIR=./node_modules/.cache/turbo turbo run build",
    "build:js": "TURBO_FORCE=true TURBO_REMOTE_CACHE_DISABLED=true TURBO_CACHE_DIR=./node_modules/.cache/turbo turbo run build:js",
    "build:types": "TURBO_FORCE=true TURBO_REMOTE_CACHE_DISABLED=true TURBO_CACHE_DIR=./node_modules/.cache/turbo turbo run build:types",
    "build:watch": "TURBO_FORCE=true TURBO_REMOTE_CACHE_DISABLED=true TURBO_CACHE_DIR=./node_modules/.cache/turbo turbo run build:watch",
    "build:local": "cd packages/core-contracts && pnpm run build",
    "test:local": "npx tsx config/testing/test-runner.ts",
    "test:unit": "TURBO_FORCE=true TURBO_REMOTE_CACHE_DISABLED=true turbo run test:ci",
    "bench": "vitest bench",
    "bench:ci": "TURBO_FORCE=true TURBO_REMOTE_CACHE_DISABLED=true turbo run bench",
    "build:ci": "TURBO_FORCE=true TURBO_REMOTE_CACHE_DISABLED=true turbo run build",
    "dev": "TURBO_FORCE=true TURBO_REMOTE_CACHE_DISABLED=true TURBO_CACHE_DIR=./node_modules/.cache/turbo turbo run dev",
    "test": "npx tsx config/testing/test-runner.ts",
    "test:e2e": "/home/boss/bin/livai-e2e-wrapper",
    "test:e2e:internal": "cd config/playwright && pnpm run test:e2e",
    "test:e2e:ui": "echo 'UI mode not yet implemented in automated script - use /home/boss/bin/livai-e2e-wrapper directly'",
    "test:e2e:debug": "echo 'Debug mode not yet implemented in automated script - use /home/boss/bin/livai-e2e-wrapper directly'",
    "test:ci": "TURBO_FORCE=true TURBO_REMOTE_CACHE_DISABLED=true turbo run test:ci",
    "type-check": "TURBO_FORCE=true TURBO_REMOTE_CACHE_DISABLED=true TURBO_CACHE_DIR=./node_modules/.cache/turbo turbo run type-check",
    "type-check:local": "npx tsc --noEmit",
    "type-check:ci": "TURBO_FORCE=true TURBO_REMOTE_CACHE_DISABLED=true turbo run type-check",
    "clean": "TURBO_FORCE=true TURBO_REMOTE_CACHE_DISABLED=true TURBO_CACHE_DIR=./node_modules/.cache/turbo turbo run clean",
    "clean:local": "node scripts/clean-local.js",
    "lint": "ESLINT_MODE=dev eslint --config eslint.config.mjs",
    "lint:dev": "ESLINT_MODE=dev eslint --config eslint.config.mjs",
    "lint:canary": "ESLINT_MODE=canary eslint --config eslint.config.mjs packages/ config/ scripts/ apps/ e2e/",
    "lint:canary:local": "ESLINT_MODE=canary npx eslint --config eslint.config.mjs packages/core-contracts/src/",
    "lint:canary:ci": "ESLINT_MODE=canary eslint --config eslint.config.mjs packages/ config/ scripts/ apps/ e2e/",
    "lint:fix": "ESLINT_MODE=dev eslint --config eslint.config.mjs --fix",
    "lint:canary:fix": "ESLINT_MODE=canary eslint --config eslint.config.mjs --fix",
    "format": "dprint fmt",
    "format:check": "dprint check",
    "quality": "pnpm run type-check && pnpm run lint:canary",
    "quality:local": "pnpm run type-check:local && pnpm run lint:canary:local",
    "type-coverage": "type-coverage packages --detail",
    "quality:ci": "pnpm run type-check:ci && pnpm run lint:canary:ci",
    "ci": "pnpm run quality:ci && pnpm run test:ci",
    "check:circular-deps": "node scripts/check-circular-deps-monorepo.js",
    "deps:unused": "node scripts/check-deps-unused.js",
    "check:dependency-policy": "node scripts/check-dependency-policy.js",
    "test:ui": "pnpm run -r test:ui",
    "test:coverage:html": "pnpm run -r test:coverage:html",
    "test:coverage:watch": "pnpm run -r test:coverage:watch",
    "coverage:open": "python3 -m webbrowser file://$(pwd)/packages/core-contracts/coverage/index.html 2>/dev/null || echo 'HTML report: packages/core-contracts/coverage/index.html'",
    "coverage:check": "node scripts/coverage-check.js",
    "coverage:file": "node scripts/coverage-file.js",
    "analyze:import-metrics": "node scripts/analyze-import-graph-metrics.js",
    "analyze:bundles": "node scripts/analyze-bundles.js",
    "analyze:bundles:size-only": "node scripts/analyze-bundles.js --size-only",
    "analyze:bundles:graph-only": "node scripts/analyze-bundles.js --graph-only",
    "analyze:bundles:compare-main": "node scripts/analyze-bundles.js --compare=main",
    "analyze:bundles:compare-none": "node scripts/analyze-bundles.js --compare=none",
    "dev:inspect": "NODE_OPTIONS='--inspect' pnpm run dev"
  },
  "devDependencies": {
    "@effect/eslint-plugin": "^0.3.2",
    "@next/eslint-plugin-next": "^16.1.1",
    "@playwright/test": "^1.57.0",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "c8": "^10.1.3",
    "cross-env": "^10.1.0",
    "depcheck": "^1.4.7",
    "detective-postcss": "^7.0.1",
    "dprint": "^0.51.1",
    "eslint": "^9.39.2",
    "eslint-formatter-compact": "^9.0.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-boundaries": "^5.3.1",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-functional": "^9.0.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-no-secrets": "^2.2.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-perf": "^3.3.3",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-security-node": "^1.1.4",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-testing-library": "^7.15.4",
    "eslint-plugin-tsdoc": "^0.5.0",
    "fast-glob": "^3.3.3",
    "globals": "^17.0.0",
    "husky": "^9.1.7",
    "istanbul-reports": "^3.2.0",
    "lint-staged": "^16.2.7",
    "rollup": "^4.55.1",
    "rollup-plugin-visualizer": "^6.0.5",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "turbo": "^2.7.3",
    "type-coverage": "^2.29.7",
    "type-fest": "^5.3.1",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^6.0.4",
    "vitest": "^4.0.17"
  },
  "overrides": {
    "@effect/rpc": "^0.73.0",
    "@effect/rpc-http": "^0.53.0",
    "@effect/vitest": "^0.28.0",
    "@effect/platform": "^0.94.0"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/sdk-node": "^0.209.0",
    "chalk": "^5.6.2",
    "dayjs": "^1.11.19"
  },
  "pnpm": {
    "overrides": {
      "@effect/platform": "0.94.1",
      "vitest": "4.0.17"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,json,md}": [
      "dprint fmt"
    ],
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
