{
  "name": "@livai/core-contracts",
  "version": "1.0.0",
  "description": "Core Contracts - Foundation layer for effects, auth, domain, errors, and infrastructure contracts",
  "main": "./dist/esm/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=24.0.0",
    "pnpm": ">=10.0.0"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "default": "./dist/esm/index.js"
    },
    "./io": {
      "types": "./dist/esm/io/index.d.ts",
      "import": "./dist/esm/io/index.js",
      "default": "./dist/esm/io/index.js"
    },
    "./fp": {
      "types": "./dist/esm/fp/index.d.ts",
      "import": "./dist/esm/fp/index.js",
      "default": "./dist/esm/fp/index.js"
    },
    "./domain": {
      "types": "./dist/esm/domain/index.d.ts",
      "import": "./dist/esm/domain/index.js",
      "default": "./dist/esm/domain/index.js"
    },
    "./errors": {
      "types": "./dist/esm/errors/index.d.ts",
      "import": "./dist/esm/errors/index.js",
      "default": "./dist/esm/errors/index.js"
    },
    "./context": {
      "types": "./dist/esm/context/index.d.ts",
      "import": "./dist/esm/context/index.js",
      "default": "./dist/esm/context/index.js"
    },
    "./targets/shared": {
      "types": "./dist/esm/targets/shared.d.ts",
      "import": "./dist/esm/targets/shared.js",
      "default": "./dist/esm/targets/shared.js"
    },
    "./targets/node": {
      "types": "./dist/esm/targets/node.d.ts",
      "import": "./dist/esm/targets/node.js",
      "default": "./dist/esm/targets/node.js"
    },
    "./targets/browser": {
      "types": "./dist/esm/targets/browser.d.ts",
      "import": "./dist/esm/targets/browser.js",
      "default": "./dist/esm/targets/browser.js"
    }
  },
  "scripts": {
    "build": "pnpm run build:types && pnpm run build:js",
    "build:js": "tsup",
    "build:types": "rm -f *.tsbuildinfo && tsc -b tsconfig.build.json && dprint fmt 'dist/esm/**/*.d.ts' --allow-no-files || true",
    "build:watch": "tsup --watch",
    "dev": "tsup --watch",
    "lint": "eslint . --config ../eslint.config.mjs --ext .ts,.tsx,.js,.jsx --cache --cache-location=.eslintcache",
    "type-check": "tsc --noEmit",
    "test": "NODE_OPTIONS='--no-warnings' vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "NODE_OPTIONS='--no-warnings' vitest run --coverage",
    "test:ci": "NODE_OPTIONS='--no-warnings' vitest run --coverage --coverage.thresholds.statements=85 --coverage.thresholds.branches=80 --coverage.thresholds.functions=85 --coverage.thresholds.lines=85",
    "clean": "rm -rf dist *.tsbuildinfo .eslintcache",
    "lint:dev": "cross-env ESLINT_MODE=dev pnpm lint",
    "lint:canary": "cross-env ESLINT_MODE=canary pnpm lint",
    "lint:fix": "cross-env ESLINT_MODE=dev pnpm lint --fix",
    "lint:ci": "cross-env ESLINT_MODE=canary pnpm lint --max-warnings 0"
  },
  "keywords": [
    "typescript",
    "fp",
    "effect",
    "domain-driven-design",
    "contracts",
    "core",
    "foundation"
  ],
  "peerDependencies": {
    "effect": "^3.19.13",
    "react": "19.2.3",
    "typescript": "5.9.3"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "effect": {
      "optional": true
    }
  },
  "devDependencies": {
    "@vitest/coverage-v8": "4.0.16",
    "eslint": "9.39.2",
    "tsup": "8.5.1",
    "typescript": "5.9.3",
    "vitest": "4.0.16"
  },
  "tags": [
    "node",
    "critical"
  ],
  "dependencies": {
    "@effect/ai": "^0.32.1",
    "@effect/ai-amazon-bedrock": "^0.12.1",
    "@effect/ai-anthropic": "^0.22.0",
    "@effect/ai-google": "^0.11.1",
    "@effect/ai-openai": "^0.36.0",
    "@effect/cli": "^0.72.1",
    "@effect/cluster": "^0.55.0",
    "@effect/experimental": "^0.57.11",
    "@effect/platform": "^0.93.8",
    "@effect/platform-browser": "^0.73.0",
    "@effect/platform-bun": "^0.86.0",
    "@effect/platform-node": "^0.103.0",
    "@effect/printer": "^0.47.0",
    "@effect/printer-ansi": "^0.47.0",
    "@effect/rpc": "^0.72.2",
    "@effect/rpc-http": "^0.52.4",
    "@effect/schema": "^0.75.5",
    "@effect/sql": "^0.48.6",
    "@effect/sql-clickhouse": "^0.45.0",
    "@effect/sql-d1": "^0.46.0",
    "@effect/sql-drizzle": "^0.47.0",
    "@effect/sql-kysely": "^0.44.0",
    "@effect/sql-libsql": "^0.38.0",
    "@effect/sql-mssql": "^0.49.0",
    "@effect/sql-mysql2": "^0.49.1",
    "@effect/sql-pg": "^0.49.7",
    "@effect/sql-sqlite-bun": "^0.49.0",
    "@effect/sql-sqlite-do": "^0.26.0",
    "@effect/sql-sqlite-node": "^0.49.1",
    "@effect/sql-sqlite-react-native": "^0.51.0",
    "@effect/sql-sqlite-wasm": "^0.49.0",
    "@effect/typeclass": "^0.38.0",
    "@effect/vitest": "^0.27.0",
    "@effect/workflow": "^0.15.2",
    "effect": "^3.19.13"
  },
  "optionalDependencies": {
    "lru-cache": "11.2.4"
  }
}
