{
  "name": "@livai/web",
  "version": "1.0.0",
  "description": "LivAi Web Application - Next.js frontend",
  "private": true,
  "type": "module",
  "engines": {
    "node": "24.12.0",
    "pnpm": "10.28.2"
  },
  "scripts": {
    "build": "next build --webpack",
    "dev": "next dev --webpack",
    "lint": "cd ../ && pnpm run lint --filter @livai/web",
    "lint:fix": "cd ../ && pnpm run lint:fix --filter @livai/web",
    "lint:ci": "cd ../ && ESLINT_MODE=canary pnpm lint --max-warnings 0 --filter @livai/web",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:coverage:html": "vitest run --coverage --coverage.reporter=html",
    "test:coverage:watch": "vitest --coverage --watch",
    "test:ci": "vitest run --coverage --coverage.thresholds.statements=70 --coverage.thresholds.branches=70 --coverage.thresholds.functions=85 --coverage.thresholds.lines=85",
    "test:e2e": "@playwright/test",
    "test:e2e:ui": "@playwright/test --ui",
    "clean": "rm -rf .next out *.tsbuildinfo .eslintcache coverage"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@livai/app": "workspace:*",
    "@livai/core-contracts": "workspace:*",
    "@livai/ui-features": "workspace:*",
    "@tanstack/react-query": "^5.90.12",
    "effect": "3.19.15",
    "next": "16.1.6",
    "next-auth": "^5.0.0-beta.30",
    "next-intl": "4.8.2",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.71.2",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "16.3.2",
    "@types/node": "25.2.3",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "@types/testing-library__jest-dom": "^6.0.0",
    "autoprefixer": "^10.4.20",
    "eslint": "9.39.2",
    "eslint-config-next": "16.1.6",
    "postcss": "^8.5.0",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^4.3.2"
  }
}
