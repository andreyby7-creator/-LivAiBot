{
  "name": "@livai/web",
  "version": "1.0.0",
  "description": "LivAi Web Application - Next.js frontend",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=24.0.0",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "cd ../ && pnpm run lint --filter @livai/web",
    "lint:fix": "cd ../ && pnpm run lint:fix --filter @livai/web",
    "lint:ci": "cd ../ && ESLINT_MODE=canary pnpm lint --max-warnings 0 --filter @livai/web",
    "type-check": "tsc --noEmit",
    "test": "NODE_OPTIONS='--no-warnings' vitest",
    "test:watch": "vitest --watch",
    "test:ui": "NODE_OPTIONS='--no-warnings' vitest --ui",
    "test:coverage": "NODE_OPTIONS='--no-warnings' vitest run --coverage",
    "test:coverage:html": "NODE_OPTIONS='--no-warnings' vitest run --coverage --coverage.reporter=html",
    "test:coverage:watch": "NODE_OPTIONS='--no-warnings' vitest --coverage --watch",
    "test:ci": "NODE_OPTIONS='--no-warnings' vitest run --coverage --coverage.thresholds.statements=70 --coverage.thresholds.branches=70 --coverage.thresholds.functions=85 --coverage.thresholds.lines=85",
    "test:e2e": "@playwright/test",
    "test:e2e:ui": "@playwright/test --ui",
    "clean": "rm -rf .next out *.tsbuildinfo .eslintcache coverage"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.12",
    "zustand": "^5.0.9",
    "zod": "^4.3.5",
    "next-auth": "^5.0.0-beta.30",
    "@hookform/resolvers": "^5.2.2",
    "react-hook-form": "^7.70.0",
    "next-i18next": "^15.4.3",
    "react-i18next": "^16.5.1",
    "effect": "3.19.13",
    "next": "16.0.0",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/react": "19.2.3",
    "@types/react-dom": "19.2.3",
    "typescript": "^5.9.3",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.5.0",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.0.0",
    "@playwright/test": "^1.57.0",
    "vite-tsconfig-paths": "^4.3.2"
  }
}
